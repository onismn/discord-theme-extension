{
  "manifest_version": 3,
  "name": "Realest Discord Themes",
  "version": "1.0.0",
  "description": "Only for broke mfs",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["content.js", "themes/sideways-ocean.js"]
    }
  ]
}
